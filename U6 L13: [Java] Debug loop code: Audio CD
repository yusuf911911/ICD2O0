import java.util.Scanner;

class Main {
  public static void main(String[] args) {

    // TASKS A

    // 1 READ the code
    // 2 PREDICT: What does the code display?
    //   Type your answer on the line below after the //
    //
    //
    // 3 RUN: now click the green Run button above.
    //

    // set up keyboard for input
    Scanner keyboard = new Scanner(System.in);

    // declare variables
    int tracks = 0;
    
    double totalMinutes = 0;
    double minutes = 0.0;
    double minuutes;

    // INPUT
    System.out.print("How many tracks? : ");
    tracks = Integer.parseInt(keyboard.nextLine());

    // INPUT PROCESS OUTPUT loop
    for (int i = 0; i < tracks; i++) { // start loop

      System.out.print("How many minutes for track " + 1);
      minutes = Double.parseDouble(keyboard.nextLine());

    } // end loop


    // PROCESS
    
    minuutes = minutes + totalMinutes;
 System.out.println("total running time: " + minuutes);

    // OUTPUT
   

  /////////// End of the tasks /////////////////
  } // end of method
} // end of class
